	  </PackageReference>
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net7.0'">
	  <PackageReference Include="Collections.Pooled">
	    <Version>1.0.82</Version>
	  </PackageReference>
	</ItemGroup>
</Project>
